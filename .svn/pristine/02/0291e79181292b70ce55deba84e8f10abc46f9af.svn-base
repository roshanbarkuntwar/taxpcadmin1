.tab-section,.main-page-heading
{
    position: fixed;
    top: 62px;
    background: #f2f2f3;
    padding-top: 7px;
    z-index:9;
}
.tab-content
{
    background: #fff;
    margin: 0px 15px;
    box-shadow: 0 2px 12px 0 rgba(44,61,101,.1);
    margin-top:10px;
