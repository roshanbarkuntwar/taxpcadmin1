.tab-section li p
{
    margin: 0px;
    display:inline-block;
}


.tab-section .nav-pills>li>a
{    
    background: #fff;
    color: $blue;
    border-radius: 0px;
    position: relative;
    font-size: 18px;
    padding: 3px 34px 3px;
    position: relative;
    font-weight: 500;
    box-shadow: 3px 2px 18px rgba(0, 0, 0, 0.12941176470588237);
}
.tab-section .nav-pills>li>a:after,
.tab-section .nav-pills>li.active>a:after
{
    margin: 0px 16px;
    position: relative;
    position: absolute;
    content: '';
    border-right: 32px solid transparent;
    top: 0;
    right: -48px;
}

.nav-pills .nav-link.active, .nav-pills .show > .nav-link {

    background-color: $blue;
}
.tab-section .nav-pills>li>a.active:after
{    


    border-bottom: 33px solid $blue;

}

.tab-section .nav-pills>li>a:after
{    

    border-bottom: 33px solid #fff;

}


.tab-section .nav-pills>li>a:absolute{
    position: absolute;
    content: '';
}


.tab-section .nav-pills>li.active>a, .tab-section .nav-pills>li.active>a:focus, .tab-section .nav-pills>li.active>a:hover
{
    background: $blue; 
    color: #fff;
}


