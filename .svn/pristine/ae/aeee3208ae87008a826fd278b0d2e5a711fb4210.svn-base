/*------carousel Style -----*/
.carousel-inner .active.left  { left: -33%;             }
.carousel-inner .active.right { left: 33%;              }
.carousel-inner .next         { left: 33%               }
.carousel-inner .prev         { left: -33%              }
.carousel-control.left        { background-image: none; }
.carousel-control.right       { background-image: none; }
.carousel-inner .item         { background: white;      }

.carousel-indicators {
	bottom:-30px
}
.carousel-indicators li {
	background-color: #4e8dff;
    width: 7px;
    height: 7px;
    border-radius: 50%;
}
.carousel-control-next-icon, .carousel-control-prev-icon {background-image:none;}
.carousel-control-next-icon:before {content: "\f105";position:absolute;font-family: fontAwesome;}
.carousel-control-prev-icon:after {content: "\f104";position:absolute;font-family: fontAwesome;}