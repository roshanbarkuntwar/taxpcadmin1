.counter{
    color: #000;
    text-align: center;
   width: 100%;
    height: 180px;
    padding: 60px 15px 20px;
    margin: 0 auto;
    border-radius: 50%;
    position: relative;
    z-index: 1;
   /* border: 1px dashed var(--main-color);*/
 /*   box-shadow: 0 0 20px -5px var(--main-color);*/
/* box-shadow: 0 0 12px -5px var(--main-color);*/
 margin-bottom: 20px;
 background-color:#e4ecfb;
/* height: 90%;*/
} 
.counter:before{
    content: '';
    border-radius: inherit; 
    border: 3px solid var(--main-color);
    position: absolute;
    top: 8px;
    left: 8px;
    right: 8px;
    bottom: 8px;
    z-index: -1;
}
.counter .counter-icon{
    color: #fff;
    background: var(--main-color);
    font-size: 30px;
    line-height: 60px;
    width: 60px;
    height: 60px;
    margin: 0 0 15px;
    border-radius: 50%;
    position: absolute;
    left: 0;
    top: 0;
}
.counter h3{
    font-size: 16px;
    font-weight: normal;
    letter-spacing: 0.5px;
    /*text-transform: uppercase;*/
    margin: 0 0 12px;
}
.counter .counter-value{
    font-size: 30px;
    font-weight: normal;
    display: block;
}
.counter.blue{ --main-color: #4e8dff; }
@media screen and (max-width:990px){
    .counter{ margin-bottom: 40px; }
