.dashboard-container 
{
    width: 90%;
    margin: auto;
    margin-top: 38px;
}



.dashboard-card-row
{
    justify-content: center;
    height: 100%; 
    display: flex;
    margin-top: 12px;
    .col-md-3 {
	margin-bottom:15px;
}
}

.dashboard-icon-wrap:nth-child(even){
  background: linear-gradient(45deg, #fffe03, #4e8dff);
}
.dashboard-card
{
    box-shadow: 1px 1px 20px 0px rgba(0, 0, 0, 0.05);
    padding: 0;
    border-radius: 15px;
    cursor: pointer;
    border-bottom: 5px solid transparent;
    min-height: 100px;
    height: 100%;
    background: #fff;
    overflow: hidden;
    text-align: center;
    position: relative;
    padding: 0px 10px 0px 10px;
   
}
.dashboard-card:nth-child(odd) {
      border-bottom: 3px solid #2c5aad !important;
}
.dashboard-card-header
{
      height: 65px;
    position: relative;
    padding-top: 18px;
}

.dashboard-card-header:after
{
    position: absolute;
    content: '';
    width: 20px;
    height: 44%;
    background: white;
    bottom: -7px;
    right: 28%;
    transform: rotate(19deg);
    width: 69px;
    height: 69px;
    background: rgba(116, 166, 255, 0.37);
    top: 54%;
    right: -22%;
    border-radius: 0px 0px 25px 16px;
    transform: rotate(148deg);
}

.dashboard-card-header:before
{
    position: absolute;
    content: '';
    width: 107px;
    height: 131px;
    background: rgba(116, 166, 255, 0.3);
    top: -147%;
    right: -32%;
    border-radius: 0px 0px 25px 16px;
    transform: rotate(45deg);
}

.dashboard-card-title 
{

    font-size: 15px;
    a{
        color: #333d50;
    }
}
.dashboard-icon-wrap
{
    width: 35px;
    height: 35px;
    background: linear-gradient(45deg, #092559, #4e8dff);
    left: 60px;
    left: 60px;
    border-radius: 12px;
    transform: rotate(45deg);
    line-height: 33px;
    display: inline-block;
}
.dashboard-icon-wrap img
{
       width: 19px;
    color: #fff;
    transform: rotate(-45deg);
}
