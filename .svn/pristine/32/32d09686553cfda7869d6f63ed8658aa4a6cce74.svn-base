					<div class="logo text-center">
						<a> <img src="./static/images/taxcpcAdmin-logo.png"
							id="loginPageLogo" class="company-logo mb-3" alt="taxcpc-logo">
						</a>
						<p class="text-center font-weight-bold mb-3" id="logoTagline"
							style="color: #2d62c1;"></p>

					</div>

					<div th:if="${errorMessage != null && !errorMessage.isEmpty()}" class="text-center col-md-12" id="errorMsgDiv">
						<div class="d-inline-block">
							<div class="form-validation p-1 my-1">
								<div class="form-validation form-validation--error p-1 my-1">
									<h5 class="d-inline-block" id="errorMsg">
										<ul class="errorMessage">
											<li><span>Login Id Or Password Is Incorrect</span></li>
										</ul>
									</h5>
								</div>
							</div>
						</div>
					</div>

					<form method="post" th:action="@{/app-login}" class="pl-3"
						autocomplete="off" >
						
						

						<div class="row">
							<div class="col-xl-8 offset-xl-2">
							<!-- <h2 class="font-weight-bold text-center">Welcome !</h2> -->
						<p>Login to your account</p>
						<div class="form-group mb-4">

							<label for="login_id" class="mb-2">Login Id<span class="font-weight-bold text-danger ml-1">*</span></label><br> <input
								type="text" id="usernameCode" placeholder="Enter Login Id"
								name="username" class="form-control text-left" required />

							
						</div>
						<div class="form-group mb-4">

							<label for="login_pwd" class="mb-2">Password<span class="font-weight-bold text-danger ml-1">*</span></label> <br> <input
								type="password" id="password" placeholder="Enter Login Password"
								name="password" class="form-control text-left" required />
						</div>

						<div class="form-group mb-4">
							<div class="text-left">
							</div>
						</div>
						<div class="form-group mt-5 mb-3 text-center">
							<button type="submit"
								class="btn btn-primary  login-btn border-0 p-2 font-weight-bold"
								>Login</button>
						</div>
						
						<div class="form-group mt-2 mb-3 text-center">
							<a for="Forget Password"  class="font-weight-bold ml-1 text-dark cursor-pointer" onclick="OpenResetpassword();">Forget Password?<span ><i data-toggle="modal" data-target="#myModal"></i></span></a>
						</div>
							</div>
						</div>
						

					</form>
				  
					<!-- <label class="float-right"><label>Powered By <img
