/*Quick Doc Dash starts*/
.sticky-container .sticky {
    position: relative;
    height: 320px;
    width:100%;
    display: inline-block;
    margin-bottom: 25px;
    box-sizing: border-box;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    font-size: 24px;
    animation: stickAnim 0.3s ease;
}
.sticky-container .sticky .sticky-body {
 padding: 8px;
  background: #fff0b0;
 // border: 1px solid #ffe168;
}
.sticky-container .sticky .sticky-body .scroll{
	overflow-y: auto;
	padding: 8px;
 	height: 280px;
}
.sticky-container .sticky .sticky-body p {
	font-size:14px;
	text-align:justify;
	word-break: break-word;
}
.sticky-container .sticky .sticky-footer {
    background: #ffe168;
    padding: 0 8px;
    //border: 1px solid #ffe168;
}
.sticky-container .sticky .sticky-footer h4{
	font-size: 17px;
	margin-bottom:0;
	color: #886e02;
}
.sticky-container .sticky .sticky-footer span {
    font-size:16px;
    cursor: pointer;
    color:#4e8dff;
}
.sticky-container .sticky .sticky-footer span:hover {
	transform:scale(1.1)
}
