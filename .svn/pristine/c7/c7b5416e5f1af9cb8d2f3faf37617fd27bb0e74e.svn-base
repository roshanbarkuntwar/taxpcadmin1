
<html lang="en" xmlns:th="http://www.thymeleaf.org" xmlns:layout="http://www.ultraq.net.nz/thymeleaf/layout" layout:decorator="layout" th:with="title='Hello, template'">
<div th:fragment="ajaxUserList">
	
	      	<table class="table table-bordered table-striped mb-1" id="table">
		        <thead>
		          <tr class="text-center">
		          <th class="th-action text-center">Action</th>
		          <th>Sr. No.</th>
		          <!--   <th class="th-action">Action</th> -->
		            <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  User Name</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th>
		            <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  User Type</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i></div>
		              </div>
		            </th>
		             <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  Login Id</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th>
		             <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  Role</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th>
		             <!-- <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  Department</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th> -->
		            <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  User Mode</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th>
		           
		            <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  Email</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th>
		            <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  Phone No.</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th>
		            
		             <th class="header">
		              <div class="table-head-inner">
		                <div class="table-heading">
		                  Status</div>
		                <div class="sort-icon"> <i class="fa fa-sort-desc"></i> <i class="fa fa-sort-asc"></i> </div>
		              </div>
		            </th>
		            
		          </tr>
		        </thead>
		        <tbody>
	          		<tr th:if="${userMastGrid != null && !userMastGrid.isEmpty()}" th:each="dataGrid,index_count : ${userMastGrid}" th:id="${'tr-'+index_count.count}">
						<td class="td-action text-center">
						<div class="custom-control custom-checkbox action-check checkbox-mt">
                                  <input type="checkbox" class="custom-control-input toggle-action-section" name="actionCheckbox" th:id="'action~'+${index_count.count}+'~'+${dataGrid.user_code}" onclick="openActionDiv(this.id);">
                                  <label class="custom-control-label" th:for="'action~'+${index_count.count}+'~'+${dataGrid.user_code}"></label>
                              </div>
                              </td>
                   <td th:text="(${dataGridDTO.paginator.pageNumber } * 10 +${index_count.count}) - 10" class="text-center"></td> 
                              
					 <!--	<td th:text="${index_count.count}"></td>
			            <td class="text-center">
			                <i class="fa edit text-primary cursor-pointer mr-1" th:id="'edit~'+${index_count.count}+'~'+${dataGrid.user_code}"  th:onclick="'editUser(this.id);'" title="Edit"></i>
		                 	<i class="fa delete text-danger cursor-pointer" th:id="'delete~'+${index_count.count}+'~'+${dataGrid.user_code}"  th:onclick="'deleteUser(this.id);'" title="Delete"></i>
			            </td> -->
			            <td th:text="${dataGrid.user_name}"></td>
			            <td class="text-center" th:text="${dataGrid.user_type}"></td>
			            <td th:text="${dataGrid.loginId}"></td>
			            <td>
			            	<th:block th:if="${roleList != null && dataGrid.role_code != null}" th:each="role : ${roleList.entrySet()}" >
								<span th:text="${dataGrid.role_code.equalsIgnoreCase(role.key) ? role.value : ''}"></span>
							</th:block>
			            </td>
			         <!--  <td>
			            	<th:block th:if="${deptList != null && dataGrid.dept_str != null}" th:each="dept : ${deptList.entrySet()}" >
								<span th:text="${dataGrid.dept_str.equals(dept.key) ? dept.value : ''}"></span>
							</th:block>
			            </td> -->
			            <td class="text-center" th:text="${dataGrid.user_mode != null && dataGrid.user_mode == 'T'} ? 'Team' : 'User'"></td>
			           
			            <td th:text="${dataGrid.email}"></td>
			            <td class="text-center" th:text="${dataGrid.phone_no}"></td>
			            <td class="text-center">
								<span th:class="${dataGrid.user_status == 'A'} ? 'text-success' : 'text-danger'" th:text="${dataGrid.user_status == 'A'} ? 'Active' : 'Inactive'"></span>
								</td>
								
			
				<input type="hidden" id="totalrecordsgrid"
					th:value="${dataGridDTO.paginator.totalRecords}">
				<input type="hidden" id="totalPagesgrid"
					th:value="${dataGridDTO.paginator.totalPages}">
				<input type="hidden" id="pageNumbergrid"
					th:value="${dataGridDTO.paginator.pageNumber}">
				<input type="hidden" id="recordsPerPageSelectgrid"
					th:value="${dataGridDTO.paginator.recordsPerPage}">
		          	</tr>
		          	<tr th:unless="${userMastGrid != null && !userMastGrid.isEmpty()}">
						<td colspan="11" class="text-center">No Record Found </td>			
					</tr>
		        </tbody>
      		</table>
	  
</div>
 
</html>