<html lang="en" xmlns:th="http://www.thymeleaf.org"
	xmlns:layout="http://www.ultraq.net.nz/thymeleaf/layout"
	layout:decorator="layout" th:with="title='Hello, template'">

<th:block layout:fragment="content">
	<link href="./static/stylesheets/projectDictionary/projectDictionary.css"
		rel="stylesheet" />
	<div class="right-section"><div class="header-gap"></div>
		<div class="header-gap"></div>
		<div class="tab-section col-md-12">
			<ul class="nav nav-pills">
				<li class="nav-item mr-5"><a class="nav-link active"
					data-toggle="pill" href="#"><p>Project Dictionary Dashboard</p>
				</a></li>
				<li class="nav-item"
					onclick="window.location.href='./projectDictionaryEntry'"><a
					class="nav-link" data-toggle="pill" href="#"><p>Project
							Dictionary Entry</p></a></li>
			</ul>
			<div class="clearfix"></div>
		</div>
		<input type="hidden" id="filetype1" name="filetype1" th:value="${filetype}"/>
		<div class="clearfix"></div>
		<div class="tab-content form-wrapper px-4 py-4">
			<div class="project-details-wrapper pt-4" th:if="${projectDictList != null && !projectDictList.isEmpty()}" th:each="dataGrid,index_count : ${projectDictList}">
				<div class="project-details" >
					<div class="project-name">
						<!-- <h5 title="Project Name" th:text="${dataGrid.project_code != null ? dataGrid.project_code : ''}"></h5> -->
							<h5> <span th:each="project : ${projectname.entrySet()}"  th:if="${project.key} == ${dataGrid.project_code}" th:text="${project.value}">
					</span> </h5>
					</div>
					<div class="project-info">
						<div class="float-left">
							<p class="project-title" title="Project Title" th:text="${dataGrid.keyword_title_question != null ? 'Title : '+dataGrid.keyword_title_question : 'Title : '}"></p>
						</div>
						<div class="collapsible float-right" data-toggle="collapse"
							th:data-target="'#demo'+${index_count.count}">
							<i class="fa fa-chevron-down"></i>
						</div>
						<div class="clearfix"></div>
					</div>
					<div class="clearfix"></div>
					<div th:id="'demo'+${index_count.count}" class="collapse p-2 text-right">
						<span class="mr-2" th:attr="onclick=|editDictionary('${dataGrid.seq_id}')|"><i class="fa fa-pencil edit-button"
							title="Edit"></i></span> 
						<span class="mr-2" th:attr="onclick=|deleteDictionary('${dataGrid.seq_id}')|"><i
							class="fa fa-trash delete-button" title="Delete"></i></span> 
						<span class="mr-2" th:attr="onclick=|viewDictionary('${dataGrid.seq_id}')|"><i class="fa fa-eye view-button" title="View"
							data-toggle="modal" data-target="#detail-modal"></i></span>
					</div>
				</div>
			</div>
			
		</div>
		
