th,td {text-align: left;}
td {padding:5px !important;}
.sr-no {
	width: 40px;
    min-width: 40px;
    max-width: 40px;
}
.doc-type {
	width: 190px;
    min-width: 190px;
    max-width: 190px;
}
.td-action {
	width: 30px;
    min-width: 30px;
    max-width: 30px;
}
.link {
	width: 72px;
